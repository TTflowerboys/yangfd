{
  "name": "currant",
  "version": "0.0.1",
  "description": "currant",
  "scripts": {
    "debug": "npm install && gulp clean && gulp debug",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build_dev_locally": "npm install --production && npm run i18n && gulp build --env dev",
    "build_dev": "gulp clean && gulp build --env dev --cdn http:\\/\\/static.yangfd.com/static/dev",
    "build_test": "npm install --production && npm run i18n && gulp build --env test --cdn http:\\/\\/static.yangfd.com/static/test",
    "build_production": "npm install --production && npm run i18n && gulp build --env production --cdn http:\\/\\/static.yangfd.com/static/production",
    "styleguideinit": "kss-node --init",
    "styleguide": "kss-node ./src/static/styles/ --less ./src/static/styles/styleguide.less --template ./.styleguide-template/",
    "theme": "gulp styleless2css",
    "sprite": "gulp sprite",
    "setupCDN": "gulp setupCDN",
    "i18n": "node tools/generate_i18n.js 'src/static/admin/**/*.{js,html}' 'src/partials/i18n_admin.html'; node tools/generate_i18n.js 'src/{,masters/**/,partials/**/,static/scripts/**/,static/templates/**/}*.{html,js}' 'src/partials/i18n.html'",
    "readme": "node tools/generate_readme.js 'README.md' 'README.html'"
  },
  "repository": {
    "type": "git",
    "url": "git@git.bbtechgroup.com:currant/currant.git"
  },
  "author": "Currant Team",
  "license": "ISC",
  "dependencies": {
    "async": "^0.9.0",
    "bower": "^1.4.1",
    "browserify": "~4.1.5",
    "chalk": "~0.5.1",
    "dict": "^1.4.0",
    "es5-shim": "~4.0.3",
    "glob": "^4.0.6",
    "glob-stream": "^3.1.15",
    "globule": "^0.2.0",
    "gulp": "~3.8.6",
    "gulp-autoprefixer": "2.3.1",
    "gulp-bower": "0.0.10",
    "gulp-browserify": "~0.5.0",
    "gulp-cached": "~1.0.1",
    "gulp-changed": "~0.4.0",
    "gulp-connect": "~2.0.6",
    "gulp-csso": "^0.2.9",
    "gulp-debug": "^1.0.1",
    "gulp-file-include": "~0.4.0",
    "gulp-filter": "^1.0.1",
    "gulp-fingerprint": "^0.3.2",
    "gulp-footer": "~1.0.5",
    "gulp-html-extend": "https://github.com/Fykec/gulp-html-extend/archive/0.5.1.tar.gz",
    "gulp-if": "^1.2.4",
    "gulp-ignore": "~1.2.0",
    "gulp-imagemin": "^1.2.1",
    "gulp-img64": "0.0.6",
    "gulp-jshint": "~1.8.4",
    "gulp-less": "~1.3.3",
    "gulp-livereload": "~2.1.1",
    "gulp-minify-css": "~0.3.7",
    "gulp-minify-html": "~0.1.4",
    "gulp-newer": "~0.3.0",
    "gulp-ng-annotate": "~0.3.0",
    "gulp-notify": "~1.4.0",
    "gulp-page-sprite": "https://github.com/lf2941270/gulp-page-sprite/archive/v1.0.5.tar.gz",
    "gulp-plumber": "~0.6.4",
    "gulp-preprocess": "^1.1.1",
    "gulp-remember": "~0.2.0",
    "gulp-rename": "~1.2.0",
    "gulp-replace": "^0.5.3",
    "gulp-rev": "^1.1.0",
    "gulp-rev-replace": "^0.3.1",
    "gulp-rimraf": "~0.1.0",
    "gulp-sourcemaps": "~1.1.4",
    "gulp-sym": "0.0.13",
    "gulp-through": "~0.3.0",
    "gulp-uglify": "~0.3.1",
    "gulp-usemin": "0.3.8",
    "gulp-useref": "^1.0.1",
    "gulp-util": "~3.0.0",
    "gulp.spritesmith": "^3.0.0",
    "jquery": "^2.1.1",
    "jsdom": "^7.2.0",
    "jshint-stylish": "~0.4.0",
    "mkdirp": "^0.5.0",
    "through2": "^0.6.3",
    "underscore": "~1.7.0",
    "wait.for": "^0.6.6",
    "yargs": "^3.6.0"
  },
  "devDependencies": {
    "bootstrap": "~3.2.0",
    "kss": "~1.0.0",
    "marked": "^0.3.2",
    "normalize.css": "~3.0.1"
  }
}
